SECRET_KEY=secret
# IMPORTANT: Change this to a strong random secret key in production
# Used for JWT tokens and OAuth session management
DEBUG=True
MODEL_PATH=./ml/model/
MODEL_NAME=model.pkl
MEMOIZATION_FLAG=True

# Database Configuration
# PostgreSQL (Recommended for production - use asyncpg driver)
DATABASE_URL=postgresql+asyncpg://user:password@host:5432/dbname?sslmode=require
# SQLite (For local development only)
# DATABASE_URL=sqlite:///./data/app.db

# OAuth Configuration
# Google OAuth (Required for Gmail integration and Google Sign-In)
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=http://localhost:8000/api/auth/google/callback

# Frontend URL (for OAuth redirects after authentication)
FRONTEND_URL=http://localhost:3000

# GitHub OAuth (Not yet implemented)
GITHUB_CLIENT_ID=your_github_client_id_here
GITHUB_CLIENT_SECRET=your_github_client_secret_here

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080,http://127.0.0.1:3000

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o

# Composio Configuration
COMPOSIO_API_KEY=your_composio_api_key_here

# Web Search Configuration
BING_API_KEY=your_bing_api_key_here
# SERPAPI_API_KEY=your_serpapi_api_key_here

# Vector Database Configuration (for RAG)
# Supermemory or Pinecone
VECTOR_DB_TYPE=supermemory
# PINECONE_API_KEY=your_pinecone_api_key_here
# PINECONE_ENVIRONMENT=your_pinecone_environment_here
# PINECONE_INDEX_NAME=armada-den-memory

# Supabase Configuration (for chat media storage)
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_KEY=your_supabase_anon_key_here
SUPABASE_BUCKET=chat-media

# FCM Configuration (for push notifications)
# Get your Firebase service account key from Firebase Console
# Set the path to your service account JSON file
FIREBASE_SERVICE_ACCOUNT_KEY_PATH=path/to/your/firebase-service-account-key.json
